{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        }
      },
      "files-metadata-get-all": {
        "ClusterId": "files-metadata-cluster",
        "Match": {
          "Path": "/api/files",
          "Methods": [ "GET" ]
        }
      },
      "files-metadata-validate": {
        "ClusterId": "files-metadata-cluster",
        "Match": {
          "Path": "/api/files/{fileId}/validate-ownership",
          "Methods": [ "POST" ]
        }
      },
      "files-storage-upload": {
        "ClusterId": "files-storage-cluster",
        "Match": {
          "Path": "/api/files/upload",
          "Methods": [ "POST" ]
        }
      },
      "files-storage-download": {
        "ClusterId": "files-storage-cluster",
        "Match": {
          "Path": "/api/files/download/{fileId}",
          "Methods": [ "GET" ]
        }
      },
      "files-storage-delete": {
        "ClusterId": "files-storage-cluster",
        "Match": {
          "Path": "/api/files/{fileId}",
          "Methods": [ "DELETE" ]
        }
      },
      "files-storage-info": {
        "ClusterId": "files-storage-cluster",
        "Match": {
          "Path": "/api/files/{fileId}/info",
          "Methods": [ "GET" ]
        }
      },
      "files-metadata-get-single": {
        "ClusterId": "files-metadata-cluster",
        "Match": {
          "Path": "/api/files/{fileId}",
          "Methods": [ "GET" ]
        }
      },
      "files-metadata-delete": {
        "ClusterId": "files-metadata-cluster",
        "Match": {
          "Path": "/api/files/{fileId}",
          "Methods": [ "DELETE" ]
        }
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-server": {
            "Address": "http://auth-service:5001/"
          }
        }
      },
      "files-metadata-cluster": {
        "Destinations": {
          "files-metadata-server": {
            "Address": "http://filemetadata-service:5002/"
          }
        }
      },
      "files-storage-cluster": {
        "Destinations": {
          "files-storage-server": {
            "Address": "http://filestorage-service:5003/"
          }
        }
      }
    }
  },
  "Services": {
    "Auth": "http://auth-service:5001",
    "FileMetadata": "http://filemetadata-service:5002",
    "FileStorage": "http://filestorage-service:5003"
  }
}